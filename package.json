{
  "name": "fast-ftp-site-sync",
  "version": "1.1.0",
  "description": "GitHub Action to sync files to FTP/SFTP remote sites",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.js -o dist --source-map --license licenses.txt",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "prepare": "npm run build",
    "test:integration:local": "./test/integration-test.sh",
    "test:integration:cleanup": "docker compose -f docker-compose.yml down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raycoarana/fast-ftp-site-sync.git"
  },
  "keywords": [
    "github-action",
    "ftp",
    "sftp",
    "sync",
    "deploy",
    "upload"
  ],
  "author": "raycoarana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raycoarana/fast-ftp-site-sync/issues"
  },
  "homepage": "https://github.com/raycoarana/fast-ftp-site-sync#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "basic-ftp": "^5.0.4",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "ssh2-sftp-client": "^10.0.3"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
